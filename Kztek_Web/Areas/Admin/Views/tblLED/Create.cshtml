@model Kztek_Model.Models.tblLED_Submit

@await Html.PartialAsync("Partial_Action", new Kztek_Library.Models.PartialActionModel<Kztek_Model.Models.tblLED_Submit>()
{
    Model = Model,
    Controller = "tblLED",
    Action = "Create",
    Page = 1,
    Data_Select_2 = (Kztek_Library.Models.SelectListModel_Chosen)ViewBag.LedFunction,


});
@section scripts{
   
    <script>
        $(document).ready(function () {
            $("#FunctionLed").change(function () {
                var vlue = $(this).val();
                $('#function_LED').val(vlue);

            });
            $("#Countinue").click(function () {
                $("#SaveAndCountinue").val('true');
            });

            $("#BackToList").click(function () {
                $("#SaveAndCountinue").val('false');
            });
         
        });
    </script>
}
